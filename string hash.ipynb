    "import hashlib\n",
    "\n",
    "#given a string, hash it!\n",
    "def string_hash(string):\n",
    "    h=hashlib.sha256()\n",
    "    h.update(string)\n",
    "    result=h.hexdigest()\n",
    "    return result"
